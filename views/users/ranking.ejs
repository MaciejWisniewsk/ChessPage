<% layout('layouts/boilerplate')%>
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Username</th>
            <th scope="col">Score</th>
        </tr>
    </thead>
    <tbody>
        <% ranking.forEach((user,index) => { %>
        <tr>
            <th scope="row"><%= index+1 %></th>
            <td><%= user.username%></td>
            <td><%= user.points%> </td>
        </tr>
        <% }) %>
    </tbody>
</table>